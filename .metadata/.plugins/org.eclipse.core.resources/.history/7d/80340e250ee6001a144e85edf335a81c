package JavaTuts;

import java.util.HashMap;

public class MapExample {

	public static void main(String[] args) {
		//
		HashMap<Integer, Integer> hm = new HashMap<Integer, Integer>();
		hm.put(0, 123);
		hm.put(1, 123);
		hm.put(22, 123);
		hm.put(3, 123);
		System.out.println(hm.get(2));
		
	}
}
